import javax.swing.*;
import java.io.*;
import java.util.*;


class acccountHolder
{
  private int accountID;
  private String address;
  
  public accountHolder()
  {
  }
  
  public void setAccountID(int id)
  {
    accountID = id;
  }
  
  public void setAddress(string add)
  {
    Address = add:
  }
  
  public int getAccountID()
  {
    return accountID;
  }
  
  public int getAddress()
  {
    return address;
  }
}
